<!-- SECTION HEAD -->
<section class="s-head">

     <div class="s-head__container-1 container">
          <div class="logo">
               установочный центр AVTOLIKE.RU
          </div>
          <nav class="main-nav">
               <ul class="main-nav__list">
                    <li class="main-nav__item">
                         <a class="main-nav__link" href="#s-results">
                              <span class="main-nav__anchor">Резульаты установки ксенона</span>
                         </a>
                    </li>
                    <li class="main-nav__item">
                         <a class="main-nav__link" href="#s-service">
                              <span class="main-nav__anchor">Какие услуги мы предлагаем</span>
                         </a>
                    </li>
                    <li class="main-nav__item">
                         <a class="main-nav__link" href="#s-install">
                              <span class="main-nav__anchor">Какие авто мы тюнингуем</span>
                         </a>
                    </li>
                    <li class="main-nav__item">
                         <a class="main-nav__link" href="#s-tune">
                              <span class="main-nav__anchor">Установка парктроника с гарантией</span>
                         </a>
                    </li>
               </ul>
          </nav>
          <div class="contacts">
               <div class="contacts__map">
                    <span class="contacts__map-address">Боенский проезд, 11с1</span>
                    <a class="contacts__map-link" href="#s-map">
                         <button class="contacts__map-button blue" type="button">Посмотреть на карте</button>
                    </a>
               </div>
               <div class="contacts__phone">
                    <span class="contacts__phone-digits">8 495 662 9947</span>
                    <button class="contacts__phone-button blue" type="button" data-action="build-modal"
                            data-modal-heading="Заказ обратного звонка" data-modal-inputs="name,phone">
                         Заказать звонок
                    </button>
               </div>
          </div>
     </div>


     <div class="s-head__container-2 container">
          <div class="s-head__heading">
               Установка биксеноновых линз
          </div>

          <div class="descriptor">
               <p class="descriptor__text">
                    Сделаем свет вашего автомобиля в 2 раза лучше за 4 часа
               </p>
               <p class="descriptor__guarantee">
                    Вернем деньги если не почувствуете результата
               </p>
          </div>
          <div class="hurryup">
               <div class="countdown" data-countdown-name="countdown-parking-capture"
                    data-countdown-lapse="310000">
                    <div class="countdown__item countdown__item--days">
                         <div class="countdown__digit">
                              &nbsp;
                         </div>
                         <div class="countdown__label">
                              дней
                         </div>
                    </div>
                    <div class="countdown__item countdown__item--hours">
                         <div class="countdown__digit">
                              &nbsp;
                         </div>
                         <div class="countdown__label">
                              часов
                         </div>
                    </div>
                    <div class="countdown__item countdown__item--minutes">
                         <div class="countdown__digit">
                              &nbsp;
                         </div>
                         <div class="countdown__label">
                              минут
                         </div>
                    </div>
                    <div class="countdown__item countdown__item--seconds">
                         <div class="countdown__digit">
                              &nbsp;
                         </div>
                         <div class="countdown__label">
                              секунд
                         </div>
                    </div>
               </div>
               <div class="form">
                    <form class="form__form">
                         <div class="form__heading m-heading js-heading">
                              Заявка на получение скидки 10%
                         </div>
                         <input class="form__input" name="name" type="text" placeholder="Имя"/>
                         <input class="form__input" name="phone" type="text" placeholder="Телефон"/>
                         <button class="form__submit blue" type="button" data-action="validate-form">
                              Оставить заявку
                         </button>
                    </form>

               </div>
          </div>
     </div>
</section>