.s-head
  height: 100%
  position: relative
  color #fff
  background url("../img/xenon/xenon-bgset-rescaled-q40.jpg") center top no-repeat
  background-attachment fixed
  overflow hidden

  &__container-1
    padding 3.25em 0 5em

.logo
  display: inline-block
  width: 11em
  background url("data:image/png;base64,") center top no-repeat
  background-size 50%
  padding-top 5em
  margin-top -.833em
  font-size .75em // 12px
  color #c8c8c8
  text-align center

.main-nav
  display inline-block
  vertical-align top
  width 37.5em
  padding-left 1.875em

  &__item
    font-family GothamProMedium
    font-size .875em // 14px
    padding-left 1.43em
    display inline-block
    vertical-align top
    background url("../img/xenon/list-dot.png") 5px 5px no-repeat
    width 20em
    text-decoration-color #eee
    margin-bottom .571em

  &__anchor
    border-bottom .0125em solid #7f7f7f
    font-family GothamProMedium
    color #fff

.contacts
  display inline-block
  position absolute
  right 0
  top 3em
  text-align center

  &__phone
    font-size 1.1875em // 19px
    font-family GothamProBold
    width 10.526em
    margin-left 1em
    display inline-block

    &-button
      margin-top .933em
      width 13.33em
      font-size .789em // 15px

  &__map
    font-size 1.0626em // 17px
    font-family GothamProBold
    width 11.76em
    display inline-block

    &-address
      display block
      margin-bottom .2em

    &-button
      margin-top 1em
      width 14.285em
      font-size .882em // 14px



.descriptor
  margin 0
  padding 0
  text-align center
  width 100%
  background none

  &__heading
    font-family GothamProLight
    font-size 3.1875em // 51px

  &__text
    font-family GothamProLight
    font-size 1.25em // 20px

    &-1
      font-family GothamProMedium
      font-size 1.3125em // 21 px
      margin 1.2em 0 1.429em 0

    &-2
      font-size 1.0625em // 17px
      margin-top .765em
      padding 0 8.824em

  &__hr
    display block
    margin 0 auto
    height .125em
    width 4em
    border-radius .2em

.capture
  margin 10.625em auto 4em
  width 50em
  padding 1.875em 2.5em
  border-radius .3125em
  border .8125em solid #ffcc00

  &__heading
    text-align: left;
    font-family: GothamProMedium;
    font-size: 1.3125em; // 21px
    margin-bottom: 1.4em;

  &__inputs
    display inline-block
    width 68%

  &__input
    width: 47%;
    margin-right: 2%;
    padding: .625em 1.25em

  &__controls
    width 30%
    display inline-block

  &__controls
    width: 30%;
    margin-right: 1%;

  &__submit
    width: 100%


@media all and (max-width: 1170px)
  .s-head
    &__container-1
      width 100%
      padding 2em 2em 4em

    &__container-2
      width 100%
      padding 0 2em

  .logo
    margin-top .8em

  .main-nav
    display none

  .contacts
    right 2em

  .capture
    margin-top 7em


@media all and (max-width: 1100px)
  .descriptor
    &__text
      &-2
        display none

    &__hr
      display none

  .capture
    margin-top 11em


@media all and (max-width: 920px)
  .capture
    width 100%


@media all and (max-width: 740px)
  .descriptor
    &__heading
      font-size 2.3em

  .capture
    &__inputs
      width 100%
      margin-bottom .5em

    &__input
      width 48%
      margin-right 0

      &:nth-child(2)
        left 2%
        float right

    &__controls
      width 100%


@media all and (max-width: 650px)
  .s-head
    background #222

    &__container
      &-1
        padding-top 5em
      &-2
        padding 0

  .logo
    padding 1.2em 0 1.3em 5em
    margin 0
    position absolute
    top 0
    left 0
    right 0
    width 100%
    background-color #fff
    color #000
    background-size 2.5em
    background-position 2em center
    font-size 1.1em

  .contacts
    position static
    margin 1em auto 0
    display block

    &__phone
      &-button
        padding 1.2em 1em
        width 13.33em

    &__map
      &-button
        padding 1.2em 1em
        width 14.285em

  .descriptor
    &__heading
      padding 0 1em
    &__text
      &-1
        padding 0 1em

  .capture
    margin-top 5em
    width 100%;
    border-width 0
    background #007fff
    border-radius 0
    margin-bottom 0

    &__input
      font-size: 1.05em
      padding .8em .8em .8em 1.25em

    &__submit
      padding 1em 0
      font-size: 1.05em


@media all and (max-width: 560px)
  .s-head
    height auto

  .capture
    margin-top 3.5em


@media all and (max-width: 500px)
  .s-head__container-1
    padding 6em 1em 1em

  .contacts
    margin .2em 0 1.5em
    text-align center

    &__map
      display block
      width 100%

      &-address
        padding-bottom .5em

      &-link
        display none

    &__phone
      display block
      width 100%
      margin 0

      &-digits
        display block

      &-button
        width 18em

  .descriptor
    &__heading
      font-size 1.8em


@media all and (max-width: 480px)
  .descriptor
    &__heading
      font-size 1.5em
    &__text
      &-1
        font-size 1.1em

  .capture
    &__input
      width 100%
      float none
      margin-left 0


@media all and (max-width: 400px)
  .capture
    padding 2em 1.5em

@media all and (max-width: 360px)
  .s-head__container-2
    overflow hidden



