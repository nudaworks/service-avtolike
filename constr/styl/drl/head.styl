.s-head
  //height 1000px;
  position relative
  color #fff
  background url("../img/drl/drl-head-bg-q40.jpg") center center no-repeat
  background-size cover

  &__container
    &-1
      padding 3.25em 0
    &-2
      padding-bottom 2em


.logo
  float left
  width: 23em
  background url("data:image/png;base64,") left center no-repeat
  background-size contain
  padding .8em 0 .8em 6.2em
  font-size .75em // 12px
  color #c8c8c8

.contacts
  float right
  text-align left

  &__phone
    background url("data:image/png;base64,") left center no-repeat
    padding-left 2.46em
    font-size 1.1875em // 19px
    font-family GothamProBold
    margin-left 2em
    display inline-block
    vertical-align top

    &-digits
      display block

    &-link
      font-family GothamProBold
      font-size .75em // 12px
      color #ffd800
      border-bottom .083em dotted #ffd800

  &__map
    background url("data:image/png;base64,") left top no-repeat
    padding-left 2.46em
    font-size 1.0626em // 17px
    font-family GothamProBold
    display inline-block
    margin-left 2em

    &-address
      display block
      margin-bottom .2em

    &-link
      font-family GothamProBold
      font-size .75em // 12px
      color #ffd800
      border-bottom .083em dotted #ffd800



.descriptor
  padding 5em 0 2.5em
  text-align center

  &__heading
    font-family GothamProLight
    font-size 3.1875em // 51px

  &__subheading
    font-size 1.1em
    padding 1em 0


.capture
  margin 4em 0 0
  width 20em
  float right

  &__form
    padding 1.875em 2.2em
    background-color #007fff

  &__heading
    font-family GothamProBold
    font-size 1.0625em // 17px
    padding-bottom 1.6em

  &__input
    width 100%
    padding .625em 1.25em

  &__submit
    width 100%
    font-size 1.0626em // 17px
    padding .75em 0

  &__info
    padding-top .85em
    font-family GothamProLight
    font-size .825em
    color #000

  &__gift
    padding 1em 2.5em 1em 5.5em
    background: url("data:image/png;base64,") no-repeat 1.5em 1.1em


@media all and (max-width: 1170px)
  .s-head
    &__container
      &-1
        width 100%
        padding 3em 2em
      &-2
        width 100%
        padding 0 2em 2em

  .logo
    margin-top -.8em

  .main-nav
    display none

  .contacts
    right 2em


@media all and (max-width: 920px)
  .s-head
    background #333
    height auto
    padding-bottom 1em

  .logo
    padding 1.2em 0 1.3em 6em
    margin 0
    position absolute
    top 0
    left 0
    right 0
    width 100%
    background #fff url("data:image/png;base64,") 2em center no-repeat
    color #000
    background-size 2.5em
    font-size 1.1em

  .contacts
    margin 3em auto 0
    float none
    text-align center
    position static
    display block

    &__map
      margin 0
      display: inline-block
      text-align left

    &__phone
      display: inline-block
      text-align left

  .descriptor
    &__heading
      font-size 2.5em
    &__subheading
      font-size 1.2em

  .capture
    width 100%
    margin-top 3em

    &__inputs
      display block
      width 100%

    &__input
      width 32%

    &__controls
      width 30%

@media all and (max-width: 740px)
  .descriptor
    &__heading
      font-size 2.3em

  .capture
    &__inputs
      width 100%
      margin-bottom .5em

    &__input
      width 48%
      margin-right 0

      &:nth-child(2)
        left 2%
        float right

    &__controls
      width 100%


@media all and (max-width: 650px)
  .logo
    font-size 1em

  .contacts
    position static
    margin 3em auto 0
    display block

    &__phone
      display block
      margin 1em auto 0
      width 340px
      padding-left 60px

      &-digits
        text-align left

      &-button
        padding 1.2em 1em
        width 13.33em

    &__map
      display block
      margin 0 auto
      width 340px
      padding-left 60px

      &-button
        padding 1.2em 1em
        width 14.285em

  .descriptor
    &__heading
      padding 0 1em

  .capture
    margin-top 0
    width 100%;
    border-width 0
    background #007fff
    border-radius 0
    margin-bottom 0

    &__input
      font-size: 1.05em
      padding .8em .8em .8em 1.25em

    &__submit
      padding 1em 0
      font-size: 1.05em


@media all and (max-width: 560px)
  .s-head
    height auto

@media all and (max-width: 500px)

  .contacts
    text-align center

    &__map
      display block
      width 100%
      padding-top .4em
      height 3em

      &-address
        padding-bottom .5em

      &-link
        display none

    &__phone
      display block
      width 100%
      margin 0

      &-digits
        display block

      &-button
        width 18em


@media all and (max-width: 480px)
  .s-head
    padding-bottom 0

    &__container
      &-2
        padding 0

  .descriptor
    &__heading
      font-size 1.8em

  .capture
    &__input
      width 100%
      float none

    &__gift
      padding 0 2.5em 2.5em 6.5em
      background-position 2.2em 0




