.s-equip
  background url("../img/default/default-pentasome-left-bottom-q60.jpg") left top no-repeat
  border .0125em solid #ddd

  &__container
    width 73.125em
    padding 2em 0
    margin 0 auto


  &__heading
    font-size 1.2em // 24px
    margin-top .2em
    padding .8em 0 .8em 5em
    float left
    width 19em
    background url("data:image/png;base64,") left top no-repeat


  &__list-wrap
    float right
    width 38.75em

  &__item
    display inline-block
    width 32%
    height 2.75em
    background center center no-repeat
    background-size contain

    &--1
      background-image url("data:image/jpeg;base64,")
      &:hover
        background-image url("data:image/jpeg;base64,")
    &--2
      background-image url("data:image/jpeg;base64,")
      &:hover
        background-image url("data:image/jpeg;base64,")
    &--3
      background-image url("data:image/jpeg;base64,")
      &:hover
        background-image url("data:image/jpeg;base64,")
    &--4
      background-image url("data:image/jpeg;base64,")
      &:hover
        background-image url("data:image/jpeg;base64,")
    &--5
      background-image url("data:image/jpeg;base64,")
      &:hover
        background-image url("data:image/jpeg;base64,")
    &--6
      background-image url("data:image/jpeg;base64,")
      &:hover
        background-image url("data:image/jpeg;base64,")


@media all and (max-width: 1170px)
  .s-equip
    &__container
      padding 2em
      width 100%


@media all and (max-width: 1070px)
  .s-equip
    &__heading
      float none
      width 100%
      padding .7em 0 .7em 4.3em
      margin-bottom 1em
      background-size contain

    &__list-wrap
      width 100%


@media all and (max-width: 690px)
  .s-equip
    &__container
      padding 3em 2em

    &__heading
      background-size 2.5em

    &__item
      width 49%


@media all and (max-width: 460px)
  .s-equip
    &__heading
      padding 0 0 1em
      background-image none
      text-align center


@media all and (max-width: 460px)
  .s-equip
    &__container
      padding 3em 1.5em