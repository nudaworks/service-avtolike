.s-partners
  border-bottom .125em solid #f6f6f6

  &__container
    margin 0 auto
    padding 2em 0
    width 73.125em

  &__heading
    margin-right .5em
    padding-top 2.3em
    float left
    width 5.75em
    font-family GothamProMedium
    font-size 1.25em // 20px
    text-align right
    background url("data:image/png;base64,") right top no-repeat

  &__list-wrap
    text-align right

  &__item
    display inline-block
    margin 1.875em 0 0 1.25em
    width 9.375em
    height 2.625em
    text-align right
    transition-duration .4s
    background center center no-repeat

    &--1
      background-image url("data:image/jpeg;base64,")
      :hover
        background-image url("data:image/jpeg;base64,")
    &--2
      background-image url("data:image/jpeg;base64,")
      :hover
        background-image url("data:image/jpeg;base64,")
    &--3
      background-image url("data:image/jpeg;base64,")
      :hover
        background-image url("data:image/jpeg;base64,")
    &--4
      background-image url("data:image/jpeg;base64,")
      :hover
        background-image url("data:image/jpeg;base64,")
    &--5
      background-image url("data:image/jpeg;base64,")
      :hover
        background-image url("data:image/jpeg;base64,")
    &--6
      background-image url("data:image/jpeg;base64,")
      :hover
        background-image url("data:image/jpeg;base64,")


@media all and (max-width: 1170px)
  .s-partners
    &__container
      width 100%

    &__heading
      float none
      width 15em
      background-position 1.5em .2em
      padding .5em 0 2em 5em
      text-align left
      margin 0 auto

    &__item
      margin 0
      width 16%

@media all and (max-width: 1050px)
  .s-partners
    &_heading
      font-family GothamProMedium
      font-size 1.25em // 20px
      background url("data:image/png;base64,") right top no-repeat

    &__item
      width 32%
      margin .5em 0


@media all and (max-width: 520px)
  .s-partners
    &__heading
      background none
      padding 0 0 1.5em
      text-align center
      width 100%

    &__item
      width 49%
      height: 2.8em
      background-size contain
