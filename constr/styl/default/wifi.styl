.s-wifi
  padding 4.3em
  background url("../img/default/default-bgset-rescaled-q40.jpg") center -830px no-repeat

  &__container
    width 34em
    min-height 2.5em
    margin 0 auto
    padding-left 6.875em
    background url("data:image/png;base64,") no-repeat
    font-family GothamProBold
    font-size 1.5em
    color #fff

@media all and (max-width: 1170px)
  .s-wifi
    &__container
      width 100%

@media all and (max-width: 760px)
  .s-wifi
    padding 3.5em
    &__container
      padding 0
      font-size 1.3em
      background none

@media all and (max-width: 600px)
  .s-wifi
    padding 2.5em 1.5em

    &__container
      font-size 1.1em

@media all and (max-width: 480px)
  .s-wifi
    &__container
      font-size 1em

