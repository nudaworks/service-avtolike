.s-advantages
  background #f8f8f8

  &__container
    margin 0 auto
    width 73.125em
    padding 5em 0

  &__heading
    font-family GothamProLight
    font-size 2.75em // 44px
    text-align center
    margin-bottom 1.8em

  &__list
    text-align center

  &__item
    background-position center top
    background-repeat no-repeat
    padding 6em 0 1em
    display inline-block
    vertical-align top
    min-height 11.6em
    max-width 11.22em

    &--1
      background-image url("data:image/png;base64,")
    &--2
      background-image url("data:image/png;base64,")
    &--3
      background-image url("data:image/png;base64,")
    &--4
      background-image url("data:image/png;base64,")
    &--5
      background-image url("data:image/png;base64,")
    &--6
      background-image url("data:image/png;base64,")
    &--7
      background-image url("data:image/png;base64,")
      max-width 15em
    &--8
      background-image url("data:image/png;base64,")
      max-width 15em
    &--9
      background-image url("data:image/png;base64,")
      max-width 15em
    &--10
      background-image url("data:image/png;base64,")
      max-width 15em




@media all and (max-width: 1170px)
  .s-advantages
    &__container
      width 100%

    &__heading
      padding 0 .5em
      font-size 2em

    &__list
      width 100%

    &__item
      padding 3em .5em 0 7em
      text-align left
      display inline-block
      background-position left center
      width 46%
      min-height 8em
      max-width none


@media all and (max-width: 800px)
  .s-advantages
    &__item
      padding 6em .3em 0
      text-align center
      background-position center top
      min-height 10em


@media all and (max-width: 560px)
  .s-advantages
    &__item
      padding .5em 1em 0 6em
      width 100%
      text-align left
      background-position 1em top
      background-size 4em
      height auto
      min-height 4.2em



@media all and (max-width: 470px)
  .s-advantages
    &__item
      padding .6em 1.5em
      min-height initial

      &:nth-child(odd)
        background #eee

      &:nth-child(even)
        background #f8f8f8

    &__em
      font-weight bold
      color #0061c2


