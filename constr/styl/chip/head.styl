.s-head
  height auto
  position: relative
  color #fff
  background: url("../img/chip/chip-outlaw-woman.png") right 50px no-repeat,
             url("../img/chip/chip-lamb-q40.jpg") left bottom no-repeat #000;
  overflow hidden

  &__container-1
    padding 3.25em 0 6em


.logo
  float left
  width: 23em
  background url("data:image/png;base64,") left center no-repeat
  background-size contain
  padding .8em 0 .8em 4em
  font-size .75em // 12px
  color #c8c8c8
  text-align center

.contacts
  float left
  text-align left

  &__phone
    background url("data:image/png;base64,") left center no-repeat
    padding-left 2.46em
    font-size 1.1875em // 19px
    font-family GothamProBold
    margin-left 2em
    display inline-block
    vertical-align top

    &-link
      font-family GothamProBold
      font-size .75em // 12px
      color #ffd800
      border-bottom .083em dotted #ffd800

  &__map
    background url("data:image/png;base64,") left top no-repeat
    padding-left 2.46em
    font-size 1.0626em // 17px
    font-family GothamProBold
    display inline-block
    margin-left 2em

    &-address
      display block
      margin-bottom .2em

    &-link
      font-family GothamProBold
      font-size .75em // 12px
      color #ffd800
      border-bottom .083em dotted #ffd800



.descriptor
  width 37em
  margin 0
  padding 0
  background none

  &__heading
    font-family GothamProLight
    font-size 3.1875em // 51px

  &__text
    margin-top 2em
    line-height 1.6em


.capture
  margin 12em 0 4em
  width 60em
  padding 1.875em 2.5em
  border-radius .3125em
  border .8125em solid #ffcc00

  &__heading
    text-align: left;
    font-family: GothamProMedium;
    font-size: 1.3125em; // 21px
    margin-bottom: 1.4em;

  &__inputs
    display inline-block
    width 73%

  &__input
    width: 30%;
    margin-right: 2%;
    padding: .625em 1.25em

  &__controls
    width 25%
    display inline-block

  &__submit
    width: 100%


@media all and (max-width: 1170px)
  .s-head
    &__container-1
      width 100%
      padding 2em 2em 4em

    &__container-2
      width 100%
      padding 0 2em

  .main-nav
    display none



  .descriptor
    text-align left



@media all and (max-width: 1080px)
  .s-head
    background url("../img/chip/chip-lamb-q40.jpg") left bottom no-repeat #000



  .capture
    margin 11em auto 2em
    width 90%




@media all and (max-width: 915px)
  .s-head
    background #333

    &__container
      &-1
        padding-top 5em
      &-2
        padding 0

  .logo
    padding 1.2em 0 1.3em 5em
    margin 0
    position absolute
    top 0
    left 0
    right 0
    width 100%
    background-color #fff
    color #000
    background-size 2.5em
    background-position 2em center
    font-size 1.1em

  .contacts
    margin-top 1em
    position static
    text-align center
    width 100%
    right 2em

    &__map
      margin 0
      display: inline-block
      text-align left
    &__phone
      display: inline-block
      text-align left

  .descriptor
    text-align center
    width 100%
    padding  0 2em

    &__text
      font-size 1.2em

  .capture
    margin-top 5em
    &__inputs
      display block
      width 100%

    &__input
      width 30%

    &__controls
      width 30%

@media all and (max-width: 740px)
  .descriptor
    &__heading
      font-size 2.3em

  .capture
    &__inputs
      width 100%
      margin-bottom .5em

    &__input
      width 48%
      margin-right 0

      &:nth-child(2)
        left 2%
        float right

    &__controls
      width 100%


@media all and (max-width: 650px)
  .logo
    font-size 1em

  .contacts
    position static
    margin 1em auto 0
    display block

    &__phone
      display block
      margin 1em auto 0
      width 340px
      padding-left 60px

      &-button
        padding 1.2em 1em
        width 13.33em

    &__map
      display block
      margin 0 auto
      width 340px
      padding-left 60px

      &-button
        padding 1.2em 1em
        width 14.285em

  .descriptor
    &__heading
      padding 0 1em
    &__text
      &-1
        padding 0 1em

  .capture
    margin-top 5em
    width 100%;
    border-width 0
    background #007fff
    border-radius 0
    margin-bottom 0

    &__input
      font-size: 1.05em
      padding .8em .8em .8em 1.25em

    &__submit
      padding 1em 0
      font-size: 1.05em


@media all and (max-width: 560px)
  .s-head
    height auto

  .capture
    margin-top 3.5em


@media all and (max-width: 500px)
  .s-head__container-1
    padding 6em 1em 1em

  .contacts
    margin .2em 0 1.5em
    text-align center

    &__map
      display block
      width 100%
      padding-top .4em
      height 3em

      &-address
        padding-bottom .5em

      &-link
        display none

    &__phone
      display block
      width 100%
      margin 0

      &-digits
        display block

      &-button
        width 18em

  .descriptor
    &__heading
      font-size 1.8em


@media all and (max-width: 480px)
  .descriptor
    &__heading
      font-size 1.5em
    &__text
      &-1
        font-size 1.1em

  .capture
    &__input
      width 100%
      float none
      margin-left 0


@media all and (max-width: 400px)
  .capture
    padding 2em 1.5em

@media all and (max-width: 360px)
  .s-head__container-2
    overflow hidden



