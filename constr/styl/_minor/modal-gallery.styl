.blinds {
  background-color: rgba(0,127,255, .9);
  animation: emerge .2s ease-in;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 100;
}


.blinds .controls {
  position: relative;
  background-color: #000;
  height: 3.125em;
}
.blinds .controls:after {
  display: table;
  content: '';
  clear: both;
}
.blinds .controls > div {
  float: left;
  color: #fff;
  z-index: 100;
  position: absolute;
  display: table-cell;
  vertical-align: center;
  top: 0;
  bottom: 0;
  padding-top: .8em;
}

.blinds .controls .positions {
  padding: .8em 1.5em;
  z-index: 102;
}

.blinds .controls .title {
  padding: .8em 6em;
  left: 0;
  right: 0;
  z-index: 101;
}

.blinds .controls .close {
  background: url(close_icon_base64) center center no-repeat;
  background-size: 50%;
  right: 0;
  width: 3.125em;
  cursor: pointer;
  z-index: 102;
}
.blinds .controls .close:hover {
  transform: rotate(5deg);
}

.blinds .wrap {
  background-color: rgba(255,255,255, .5);
  height: 100%;
  text-align: center;
}

.blinds .wrap .display {
  display: inline-block;
  position: relative;
  margin: 0 auto;

}

.blinds .wrap .display > div {
  display: inline-block;
}

.blinds .wrap .display .img-box {
  padding: 0 4em;
  display: table-cell;
  vertical-align: center;
}

.blinds .wrap .display .img-box img {
  max-width: 100%;
}

.blinds .wrap .display .move {
  top: 0;
  bottom: 0;
  position: absolute;
  width: 2em;
  cursor: pointer;
}
.blinds .wrap .display .move.left {
  left: .5em;
  background: url("data:image/png;base64,") left center no-repeat;
  background-size: contain;

}
.blinds .wrap .display .move.right {
  right: .5em;
  background: url("data:image/png;base64,") left center no-repeat;
  background-size: contain;
}


.blinds .wrap .display .img-box {
  height: 100%;
  width: 90%;
  margin: 0 auto;
}

